<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" 
		"http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>

	<settings>
		<setting name="logImpl" value="LOG4J" />
	</settings>

	<environments default="mysql">
		<environment id="mysql">
			<transactionManager type="JDBC" />
			<dataSource type="POOLED">
				<property name="driver" value="com.mysql.jdbc.Driver" />
				<property name="url"
					value="jdbc:mysql://127.0.0.1:3306/cloud" />
				<property name="username" value="root" />
				<property name="password" value="abc123lzf" />
			</dataSource>
		</environment>
	</environments>

	<mappers>
		<mapper class="com.cloud.entity.UserMapper" />
		<mapper class="com.cloud.entity.CloudFileMapper" />
		<mapper class="com.cloud.entity.FolderMapper" />
	</mappers>

</configuration>